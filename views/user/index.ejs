<div class="container">
	<h2>All User</h2>
	<hr />
	<table class="table table-hover table-striped table-bordered">
		<tr>
			<th>#</th>
			<th>Name</th>
		</tr>
		<% data.forEach(function(item, index){ %>
			<tr>
				<td><%= index+1 %></td>
				<td><%= item.name %></td>
			</tr>
	<% }); %>
	</table>
</div>